    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "jest": "24.9.0",
    "lerna": "3.20.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "ramda": "0.26.1",
    "react": "16.12.0",
