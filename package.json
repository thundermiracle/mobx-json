{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev:form": "lerna run dev --scope @mobx-json/form --stream",
    "dev:muiform": "lerna run dev --scope @mobx-json/mui-form --stream",
    "dev:form:exp": "lerna run dev --scope @mobx-json/form-example --stream",
    "dev:muiform:exp": "lerna run dev --scope @mobx-json/mui-form-example --stream",
    "build": "lerna run build --use-workspaces --stream",
    "eslint": "lerna run eslint:src --stream",
    "type-check": "lerna run type-check --stream",
    "jest": "jest && codecov",
    "test:q": "yarn eslint && yarn type-check && yarn jest",
    "jest:ci": "jest -i && codecov",
    "test": "yarn eslint && yarn type-check && yarn jest:ci",
    "publish": "lerna publish"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
